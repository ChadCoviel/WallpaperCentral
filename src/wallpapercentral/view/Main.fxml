<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="wallpapercentral.view.MainController"
            prefHeight="400.0" prefWidth="600.0">
    <top>
        <fx:include fx:id="menu" source="Menu.fxml"/>
    </top>
    <center>
        <SplitPane dividerPositions="0.2">
            <items>
                <fx:include fx:id="commands" source="Commands.fxml"/>
                <fx:include fx:id="content" source="Content.fxml"/>
            </items>
        </SplitPane>
    </center>
</BorderPane>
