<MenuBar fx:id="menuBar" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wallpapercentral.menu.MenuController">
  <menus>
    <Menu mnemonicParsing="false" text="File">
      <items>
        <MenuItem mnemonicParsing="false" text="Close" onAction="#close"/>
        <MenuItem mnemonicParsing="false" text="Save" onAction="#save"/>
        <MenuItem mnemonicParsing="false" text="Load" onAction="#load"/>
      </items>
    </Menu>
  </menus>
</MenuBar>

